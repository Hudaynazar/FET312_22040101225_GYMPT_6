      "cell_type": "code",
      "source": [
        "\n",
        "!pip install -q -U torch torchvision transformers peft bitsandbytes accelerate gradio qwen-vl-utils\n",
        "\n",
        "import torch\n",
        "from transformers import Qwen2VLForConditionalGeneration, AutoProcessor, BitsAndBytesConfig\n",
        "from peft import PeftModel\n",
        "import gradio as gr\n",
        "from PIL import Image\n",
        "from google.colab import drive\n",
        "\n",
        "\n",
        "drive.mount('/content/drive')\n",
        "\n",
        "print(\"Model Yükleniyor...\")\n",
        "\n",
        "base_model_id = \"Qwen/Qwen2-VL-7B-Instruct\"\n",
        "adapter_path = \"/content/drive/MyDrive/Qwen2-VL-Calorie-Model-Final/checkpoint-400\"\n",
        "\n",
        "bnb_config = BitsAndBytesConfig(\n",
        "    load_in_4bit=True,\n",
        "    bnb_4bit_quant_type=\"nf4\",\n",
        "    bnb_4bit_compute_dtype=torch.bfloat16\n",
        ")\n",
        "\n",
        "base_model = Qwen2VLForConditionalGeneration.from_pretrained(\n",
        "    base_model_id,\n",
        "    quantization_config=bnb_config,\n",
        "    device_map=\"auto\",\n",
        "    torch_dtype=torch.bfloat16\n",
        ")\n",
        "\n",
        "\n",
        "model = PeftModel.from_pretrained(base_model, adapter_path)\n",
        "model.eval()\n",
        "\n",
        "processor = AutoProcessor.from_pretrained(base_model_id, min_pixels=256*28*28, max_pixels=1280*28*28)\n",
        "\n",
        "print(\"Model Hazır! Uygulama başlatılıyor...\")\n",
        "\n",
        "\n",
        "def analyze_food(image):\n",
        "    if image is None:\n",
        "        return \"Lütfen bir yemek fotoğrafı yükleyin.\"\n",
        "\n",
        "    \n",
        "    if image.mode != \"RGB\":\n",
        "        image = image.convert(\"RGB\")\n",
        "\n",
        "    \n",
        "    \n",
        "    system_prompt = \"<|im_start|>system\\nYou are a helpful assistant.<|im_end|>\\n\"\n",
        "    \n",
        "    user_prompt = \"<|im_start|>user\\n<|vision_start|><|image_pad|><|vision_end|>Fotoğraftaki yiyecekleri listele ve her birinin kalorisini tahmin et.<|im_end|>\\n\"\n",
        "    assistant_prompt = \"<|im_start|>assistant\\n\"\n",
        "\n",
        "    full_text = system_prompt + user_prompt + assistant_prompt\n",
        "\n",
        "    \n",
        "    inputs = processor(\n",
        "        text=[full_text],\n",
        "        images=[image],\n",
        "        padding=True,\n",
        "        return_tensors=\"pt\"\n",
        "    ).to(\"cuda\")\n",
        "\n",
        "    \n",
        "    \n",
        "    with torch.no_grad():\n",
        "        generated_ids = model.generate(\n",
        "            **inputs,\n",
        "            max_new_tokens=512,\n",
        "            do_sample=False \n",
        "        )\n",
        "\n",
        "   \n",
        "    generated_ids_trimmed = [\n",
        "        out_ids[len(in_ids) :] for in_ids, out_ids in zip(inputs.input_ids, generated_ids)\n",
        "    ]\n",
        "    output_text = processor.batch_decode(\n",
        "        generated_ids_trimmed, skip_special_tokens=True, clean_up_tokenization_spaces=False\n",
        "    )\n",
        "\n",
        "    return output_text[0]\n",
        "\n",
        "\n",
        "with gr.Blocks(theme=gr.themes.Soft()) as demo:\n",
        "    gr.Markdown(\"\"\"\n",
        "   \n",
        "    **Yemeğinin fotoğrafını yükle, içindekileri ve kalorisini anında öğren!**\n",
        "    \"\"\")\n",
        "\n",
        "    with gr.Row():\n",
        "        with gr.Column():\n",
        "            img_input = gr.Image(type=\"pil\", label=\"Yemek Fotoğrafı Yükle\")\n",
        "            submit_btn = gr.Button(\"Analiz Et\", variant=\"primary\")\n",
        "\n",
        "        with gr.Column():\n",
        "            output_text = gr.Textbox(label=\"Yapay Zeka Analizi\", lines=10)\n",
        "\n",
        "    \n",
        "    submit_btn.click(fn=analyze_food, inputs=img_input, outputs=output_text)\n",
        "\n",
        "\n",
        "demo.launch(debug=True, share=True)"
      ],
